        <classes>
            <class name="com.example.demo8.cases.LoginTest">
                <methods>
                    <include name="loginTrue"></include>
                    <include name="loginFalse"></include>
                </methods>
            </class>

            <class name="com.example.demo8.cases.AddUserTest">
                <methods>
                    <include name="addUser"></include>
                </methods>
            </class>

            <class name="com.example.demo8.cases.GetUserInfoListTest">
